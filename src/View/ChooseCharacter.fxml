<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ChooseCharacterController">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <HBox alignment="CENTER" prefHeight="271.0" prefWidth="333.0">
         <children>
            <Button mnemonicParsing="false" onAction="#A" text="A" />
            <Button mnemonicParsing="false" onAction="#B" text="B" />
            <Button mnemonicParsing="false" onAction="#C" text="C" />
            <Button mnemonicParsing="false" onAction="#D" text="D" />
         </children>
      </HBox>
   </children>
</GridPane>
