<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.MyViewController">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <BorderPane prefHeight="550.0" prefWidth="700.0">
         <top>
            <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <bottom>
                  <Label text="Hello!" BorderPane.alignment="CENTER" />
               </bottom>
            </BorderPane>
         </top>
         <center>
            <Button mnemonicParsing="false" onAction="#Start" text="Start" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
